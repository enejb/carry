@import "scss/reset";
@import "scss/global";
@import "scss/layout";
@import "scss/nav";


/* Alignment */
.alignleft {
	display: inline;
	float: left;
	margin-right: 1.5em;
}
.alignright {
	display: inline;
	float: right;
	margin-left: 1.5em;
}
.aligncenter {
	clear: both;
	display: block;
	margin: 0 auto;
}

/* Text meant only for screen readers */
.assistive-text {
	clip: rect(1px 1px 1px 1px); /* IE6, IE7 */
	clip: rect(1px, 1px, 1px, 1px);
	position: absolute !important;
}



/* =Header
----------------------------------------------- */
#masthead{
	background: #3A9AC7;
	color:#FFF;
	padding: 5px 0;
}
#masthead hgroup{
	position: relative;
	overflow: hidden;
	display: block;
}
.site-title,
.site-description{
	float: left;
	clear: none;
	margin-right: 10px;
}
.site-description {
	line-height: 24px;
}
.site-title a{
	color:#FFF;
	text-decoration: none;
	font-weight: 600;
	font-size: 16px;
}


/* =Content
----------------------------------------------- */

.sticky {
}
.page .hentry{
	padding-top:15px;
}
.single .hentry{
	padding-top:15px;
}
.content-404,
.hentry {
	margin: 0 15px 1.5em 0;
	padding: 0 0 15px 15px;
	border-bottom: 1px solid #EEE;
}
.page-title{
	font-size: 16px;
	line-height: 1;
	margin-bottom: 2px;
}

.archive .page-title span{
	font-style: italic;
}
.error404 .page-title,
.page .entry-title,
.single .entry-title{
	font-size: 26px;
	line-height: 1;
	margin-bottom: 2px;
}
.entry-title a{
	font-size:16px;
	text-decoration: none;
}
.comments-area,
.page-header{
	margin: 20px 15px 5px;
}
.entry-meta {
	clear: both;
	text-transform: uppercase;
	font-size:11px;
	color:#AAA;
}
.byline {
	display: none;
}
.single .byline,
.group-blog .byline {
	display: inline;
}
.entry-content,
.entry-summary {
	margin: 1.5em 0 0;
}
.page-links {
	clear: both;
	margin: 0 0 1.5em;
}

/* =Pulse 
----------------------------------------------- */
.pulse-cpt .pulse{
	border:none;
	padding: 0;
}

/* =Asides
----------------------------------------------- */

.blog .format-aside .entry-title,
.archive .format-aside .entry-title {
	display: none;
}
/* =Widgets
----------------------------------------------- */

.widget {
	margin: 0 0 1.5em;
}
.widget ul{
 list-style: none;
 margin-left: 5px;
}
.tagcloud, 
#calendar_wrap,
.textwidget{
	padding: 0 5px;
}
.widget_categories select,
.widget_archive select{
	margin: 5px 5px 0;
}
#searchform {
	border:1px solid #CCC;
	padding: 2px 0 2px;
	margin-right: 5px;
}
#searchform #s{
	padding: 2px;
	width: 63%;
	border: 0;
}
.half #searchform #s{
	width: 74%;
}
.half #searchform #searchsubmit{
	width: 20%;
	text-overflow: ellipsis;
	padding: 3px 1%;
	float: right;
	margin-right: 1%;
}
#searchform #searchsubmit{
	display: inline;
	padding: 1px 3px;
	font-size: 11px;
}
#tertiary #searchform #s{
	width: 80%;
}
#tertiary #searchform #searchsubmit {
	width: 15%;
	text-overflow: ellipsis;
	padding: 3px 1%;
	float: right;
	margin-right: 1%;
}
#tertiary .widget_search .widget-title {
	margin-bottom: 10px;
}
#tertiary #searchform{
	margin: 0 10px 0 0;
	padding: 2px 0 3px;
}
#tertiary .postbox{
	margin: 0 5px 0 0;
}
/* rss */
.rsswidget img{
	vertical-align: middle;
}
.rss-date{
	color: #AAA;
	display: block;
	font-size: 11px;
	text-transform: uppercase;
	margin-bottom: 5px;
}
.rssSummary{
	padding-right: 10px;
	text-overflow: ellipsis;
}
/* =Widget Areas
----------------------------------------------- */
.widget .widget-title{
	background: #EEE;
	padding: 2px 10px 2px 5px;
}
.widget .menu{
	list-style: none;
	margin: 0 0 0 10px;
}
.widget li{
	padding: 2px 0;
}
/* =Secondary
----------------------------------------------- */
#secondary{
	font-size: 11px;
}


/* =Media
----------------------------------------------- */

.site-header img,
.entry-content img,
.comment-content img,
.widget img {
	max-width: 100%; /* Fluid images for posts, comments, and widgets */
}
.site-header img,
.entry-content img,
img[class*="align"],
img[class*="wp-image-"] {
	height: auto; /* Make sure images with WordPress-added height and width attributes are scaled correctly */
}
.site-header img,
.entry-content img,
img.size-full {
	max-width: 100%;
	width: auto; /* Prevent stretching of full-size images with height and width attributes in IE8 */
}
.entry-content img.wp-smiley,
.comment-content img.wp-smiley {
	border: none;
	margin-bottom: 0;
	margin-top: 0;
	padding: 0;
}
.wp-caption {
	border: 1px solid #ccc;
	max-width: 100%;
}
.wp-caption.aligncenter,
.wp-caption.alignleft,
.wp-caption.alignright {
	margin-bottom: 1.5em;
}
.wp-caption img {
	display: block;
	margin: 1.2% auto 0;
	max-width: 98%;
	height: auto;
}
.wp-caption-text {
	text-align: center;
}
.wp-caption .wp-caption-text {
	margin: 0.8075em 0;
}
#content .gallery {
	margin-bottom: 1.5em;
}
.gallery-caption {
}
#content .gallery a img {
	border: none;
	height: auto;
	max-width: 90%;
}
#content .gallery dd {
	margin: 0;
}
#content .gallery-columns-4 .gallery-item {
}
#content .gallery-columns-4 .gallery-item img {
}

/* Make sure embeds and iframes fit their containers */
embed,
iframe,
object {
	max-width: 100%;
}



/* =Comments
----------------------------------------------- */
.comments-area{
	background: #EEE;
	margin:0 10px 0 1px;
	padding: 10px 10px 1px;
}

.avatar{
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	-webkit-box-shadow: 0 1px 1px 0px rgba(0, 0, 0, 0.3);
	-moz-box-shadow: 0 1px 1px 0px rgba(0,0,0,0.3);
	-ms-box-shadow: 0 1px 1px 0px rgba(0,0,0,0.3);
	-o-box-shadow: 0 1px 1px 0px rgba(0,0,0,0.3);
	box-shadow: 0 1px 1px 0px rgba(0, 0, 0, 0.3);
}
.comment-shell{
	width: 100%;
}
.comment{
	clear: both;
	position: relative;
	overflow: hidden;
	padding: 15px 5px 0 15px;
	background: #FFF;
	
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
}
.commentlist{
	margin: 0;
	list-style: none;
}
.children {
	border-top:1px solid #DADADA;
	list-style: none;
	margin: 0 -5px 0 -15px;
	background: rgba(240,240,240,0.5);
	padding: 5px 5px 5px 15px;
	border-bottom-right-radius: 3px;
	border-bottom-left-radius: 3px;
	-webkit-border-bottom-left-radius:3px; /* bottom left corner */
	-webkit-border-bottom-right-radius:3px; /* bottom right corner */
	-moz-border-radius-bottomleft:3px; /* bottom left corner */
	-moz-border-radius-bottomright:3px; 
}
.children li.comment{
	margin-bottom: 5px;
	border:none;
}

.depth-2 > #respond,
.depth-4 > #respond,
.depth-6 > #respond,
.depth-2 > .children,
.depth-4 > .children,
.depth-6 > .children{
	background: rgba(240,240,240,0.8);
}
.depth-1 > #respond,
.depth-3 > #respond,
.depth-5 > #respond,
.depth-1 > .children,
.depth-3 > .children,
.depth-5 > .children {
	background: rgba(240,240,240,0.5);
}

.comment-author img{
	float: left;
}
.comment-author .fn{
	float: left;
	font-weight: bold;
	text-transform: capitalize;
	font-style: normal;
	margin-left: 15px;
}
.comment-shell footer em{
	color: #AAA;
	float: left;
	clear: left;
	margin: -10px 0 10px 44px;
}
.depth-1{
	margin-top: 10px;
}
.comment-meta{
	font-size:11px;
	float: right;
}
.comment-meta a{
	color:#AAA;
	text-decoration: none;
	margin-right: 2px;
}
.comment-meta a:hover{
	text-decoration: underline;
}
.comment-content{
	float: left;
	margin: -10px 0 0 45px;
	clear:left;
}
.bypostauthor {
}
/* comment from */
.logged-in-as{
	float: right;
}
.comments-area code{
	color:#AAA;
	font-size:11px;
	
}
#commentform .form-allowed-tags{
	background: #EEE;
	padding:5px 10px;
	margin-left: -10px;
	clear: none;
	position: absolute;
}
#respond{
	background: #EFEFEF;
	margin-top: 10px;
}
.commentlist #respond{
	margin: 0 -5px 0 -15px;
	padding: 5px 5px 5px 15px;
}
#commentform{
	position: relative;
	overflow: hidden;
	padding-bottom: 100px;
	background: #FFF;
	border:1px solid #CCC;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
	padding: 10px 10px 100px;
	margin-bottom: 10px;
}
.form-allowed-tags{
	position: absolute;
	bottom: 0;
}
.comment-form-comment{
	clear: both;
	display: block;
	padding-top: 10px;
}
.comment-logout{
	float: left;
	margin-left: 10px;
}
#commentform p{
	clear: both;
	position: relative;
	overflow: hidden;
}
#commentform .comment-form-email{
	clear: none;
}
.comment-form-email,
.comment-form-author{
	width: 48%;
	float: left;
}
#url{
	width: 98%;
}
#commentform label{
	display: block;
	float: left;
}
#commentform input{
	clear: both;
	float: left;
}
#commentform #submit{
	float: none;
}
#commentform .required{
	color:red;
	display: block;
	float: left;
	font-size: 16px;
	margin-left: 3px;
	vertical-align: text-top;
}
.comment-form-comment label,
.comment-form-comment textarea{
	margin: 0;
}
/* = Action links 
----------------------------------------------- */
#cancel-comment-reply-link,
.comment-logout,
.comment-edit-link,
.comment-reply-link{
	color:#333;
	text-decoration: none;
	font-size: 12px;
}
.reply{
	clear:both;
	padding-bottom: 10px;
}
#cancel-comment-reply-link,
.comment-logout:hover,
.comment-edit-link:hover,
.comment-reply-link:hover{
	text-decoration: underline;
	color:#333;
}


/* = 404 page
----------------------------------------------- */
.column-404{
 width:14.28%;
 float: left;
 height: 5px;
 margin-bottom: 16px;
}
.yellow{
	background: yellow;
}
.red{
	background: red;
}
.blue{
	background: blue;
}
.black{
	background: black;
}
.green{
	background: lime;
}
.purple{
	background: fuchsia;
}
.aqua{
	background: aqua;
}
.content-404 .widgettitle{
	text-transform: uppercase;
	color:#444;
	
}

